            "rules": {
                "stringType": "NUMBER"
            },
            "extraAttr": {
                "filters": "float"
            },
            "tip": "",
            "multiline": false
        },
        {
            "name": "image_margin",
            "displayName": "胶带图间距",
            "defaultValue": "10",
            "datatype": "HIDDEN",
            "rules": {
                "stringType": "NUMBER"
            },
            "extraAttr": {
                "filters": "float"
            },
            "tip": "",
            "multiline": false
        },
        {
            "name": "speed",
            "displayName": "胶带图片步进间隔时间(毫秒)",
            "defaultValue": "25",
            "datatype": "HIDDEN",
            "rules": {
                "stringType": "NUMBER"
            },
            "extraAttr": {
                "filters": "float"
            },
            "multiline": false
        },
        {
            "name": "options",
            "displayName": "图片项",
            "datatype": "LIST",
            "tip": "图片列表",
            "rules": {
                "required": true,
                "minCount": 4,
                "maxCount": 10
            },
            "items": [
                {
                    "name": "imgsrc",
                    "displayName": "图片地址",
                    "defaultValue": "",
                    "datatype": "UPLOAD",
                    "rules": {
                        "stringType": "URL"
                    },
                    "extraAttr": {},
                    "tip": "",
                    "rules":{
                        "required":true
                    },
                    "extraAttr":{
                        "fileType":"IMAGE",
                        "maxSize":10,
                        "width": 110,
                        "height": 90,
                        "ext":"jpg,jpeg"
                    }
                },
                {
                    "name": "imgurl_rcv_url",
                    "displayName": "图片点击URL",
                    "defaultValue": "http://",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "stringType": "URL",
                        "maxByteLength": 255
                    },
                    "tip": "图片点击URL"
                }
            ]
        },
        {
            "name": "left_arrow",
            "displayName": "向左箭头图标",
