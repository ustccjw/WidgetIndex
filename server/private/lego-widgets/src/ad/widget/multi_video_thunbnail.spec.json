{
    "contents": ["ad.widget.MultiVideoThunbnail"],
    "userPrefs": [
        {
            "name": "width",
            "displayName": "图片宽度",
            "defaultValue": "100",
            "datatype": "STRING",
            "rules": {
                "stringType": "NUMBER"
            },
            "multiline": false
        },
        {
            "name": "display_video_icon",
            "displayName": "display_video_icon",
            "defaultValue": true,
            "datatype": "BOOL",
            "rules": {
                "stringType": "BOOL"
            }
        },
        {
            "name": "display_play_button",
            "displayName": "display_play_button",
            "defaultValue": true,
            "datatype": "BOOL",
            "rules": {
                "stringType": "BOOL"
            }
        },
        {
            "name": "display_playing_tip",
            "displayName": "display_playing_tip",
            "defaultValue": true,
            "datatype": "BOOL",
            "rules": {
                "stringType": "BOOL"
            }
        },
        {
            "name": "play_btn_text",
            "displayName": "play_btn_text",
            "tip": "最多10个字节（5个汉字）",
            "datatype": "STRING",
            "rules": {
                "required": true,
                "maxByteLength": 10
            }
        },
        {
            "name": "options",
            "displayName": "图片列表",
            "datatype": "LIST",
            "items": [
                {
                    "name": "img_url",
                    "displayName": "图片地址",
                    "defaultValue": "http://www.baidu.com",
                    "datatype": "UPLOAD",
                    "rules": {
                        "stringType": "URL",
                        "required": true
                    },
                    "extraAttr": {
                        "fileType": "IMAGE",
                        "ext": "jpg,gif,png,jpeg"
                    }
                },
                {
                    "name": "rcv_url",
                    "displayName": "图片链接地址",
                    "defaultValue": "http://",
                    "datatype": "STRING",
                    "rules": {
                        "stringType": "URL"
                    },
                    "multiline": false
                },
                {
                    "name": "text",
                    "displayName": "图片说明",
                    "datatype": "STRING",
                    "multiline": false
                },
                {
                    "name": "play_btn_text",
                    "displayName": "play_btn_text",
                    "tip": "最多10个字节（5个汉字）",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 10
                    }
                }
            ]
        }
    ]
}