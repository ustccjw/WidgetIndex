{
    "contents": ["ad.widget.tieba.site.SiteSimpleHeader"],
    "userPrefs": [
        {
            "name": "name",
            "displayName": "显示吧名",
            "nameTip": "（必填）",
            "defaultValue": "%%TIEBA_NAME%%",
            "datatype": "STRING",
            "rules": {
                "required": true,
                "minByteLength": 4,
                "maxByteLength": 28,
                "regex": {
                    "pattern": "^[0-9a-zA-Z_\\u4E00-\\u9FA5]+$",
                    "errorText": "吧名限汉字、字母、数字和下划线"
                }
            },
            "extraAttr": {},
            "tip": "",
            "multiline": false
        },
        {
            "name": "logo_url",
            "displayName": "Logo图片",
            "nameTip": "（必填）",
            "defaultValue": "",
            "tip": "http://bcscdn.baidu.com/adtest/a815d63a02aa7ad89702d0bcbc301be9.gif",
            "datatype": "UPLOAD",
            "rules": {
                "required": true
            },
            "extraAttr": {
                "fileType": "IMAGE",
                "maxSize": 50,
                "width": 156,
                "height": 156,
                "ext": "jpg,png"
            }
        },
        {
            "name": "skin",
            "displayName": "背景皮肤",
            "datatype": "OBJECT",
            "items": [
                {
                    "name": "top_url",
                    "displayName": "皮肤顶图",
                    "defaultValue": "",
                    "datatype": "UPLOAD",
                    "rules": {},
                    "extraAttr": {
                        "fileType":"IMAGE",
                        "maxSize": 200,
                        "minWidth": 1280,
                        "maxWidth": 2400,
                        "height": 460,
                        "ext": "jpg,png"
                    }
                },
                {
                    "name": "bottom_repeat",
                    "displayName": "底图水平平铺",
                    "defaultValue": "true",
                    "datatype": "ALTERNATIVE",
                    "rules": {},
                    "extraAttr": {},
                    "enumValues": [
                        {
                            "value": "true",
                            "displayValue": "平铺",
                            "items": [
                                {
                                    "name": "bottom_url",
                                    "displayName": "皮肤底图",
                                    "defaultValue": "",
                                    "datatype": "UPLOAD",
                                    "rules": {},
                                    "extraAttr": {
                                        "fileType":"IMAGE",
                                        "maxSize": 50,
                                        "width": 1,
                                        "height": 860,
                                        "ext": "jpg,png"
                                    }
                                }
                            ]
                        },
                        {
                            "value": "false",
                            "displayValue": "不平铺",
                            "items": [
                                {
                                    "name": "bottom_url",
                                    "displayName": "皮肤底图",
                                    "defaultValue": "",
                                    "datatype": "UPLOAD",
                                    "rules": {},
                                    "extraAttr": {
                                        "fileType":"IMAGE",
                                        "maxSize": 200,
                                        "minWidth": 1280,
                                        "maxWidth": 2400,
                                        "ext": "jpg,png"
                                    }
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "color",
                    "displayName": "皮肤背景色",
                    "tip": "填写HEX格式色值，如：#FF0000",
                    "datatype": "STRING",
                    "rules": {
                        "regex": {
                            "pattern": "^#[\\da-f]{3}|#[\\da-f]{6}$",
                            "modifiers": "i",
                            "errorText": "色值必须为HEX格式，如：#FF0000"
                        }
                    }
                }
            ],
            "rules": {}
        },
        {
            "name": "frs_rcv_url",
            "displayName": "官吧首页地址",
            "defaultValue": "%%TIEBA_FRS_URL%%",
            "datatype": "HIDDEN",
            "rules": {
                "stringType": "URL"
            },
            "extraAttr": {}
        },
        {
            "name": "gallery",
            "displayName": "顶部专辑",
            "datatype": "LIST",
            "items": [
                {
                    "name": "image_url",
                    "displayName": "专辑封面",
                    "nameTip": "（必填）",
                    "tip": "http://bcscdn.baidu.com/adtest/fffba8dbbd9617817871f37f3681ddd3.gif",
                    "defaultValue": "",
                    "datatype": "UPLOAD",
                    "rules": {
                        "required": true
                    },
                    "extraAttr": {
                        "fileType": "IMAGE",
                        "maxSize": 50,
                        "width": 125,
                        "height": 90,
                        "ext": "jpg,png"
                    }
                },
                {
                    "name": "alt",
                    "displayName": "专辑标题",
                    "nameTip": "（必填）",
                    "defaultValue": "",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 20
                    },
                    "extraAttr": {},
                    "tip": "10个汉字（20个字节）以内",
                    "multiline": false
                },
                {
                    "name": "rcv_url",
                    "displayName": "跳转地址",
                    "defaultValue": "",
                    "datatype": "STRING",
                    "rules": {
                        "stringType": "URL"
                    },
                    "extraAttr": {},
                    "tip": "",
                    "multiline": false
                }
            ],
            "tip": "",
            "rules": {
                "minCount": 0,
                "maxCount": 4
            },
            "extraAttr": {
                "initialCount": 0
            }
        }
    ]
}