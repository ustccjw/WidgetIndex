{
    "selectors" : [
        {
            "name": ".ad-widget-table thead td",
            "displayName": "表头背景色",
            "rules": [
                {
                    "name": "background-color",
                    "displayName": "背景色",
                    "defaultValue": "#DEE6FE"
                },
                {
                    "name": "color",
                    "displayName": "字体颜色",
                    "defaultValue": "#000000"
                },
                {
                    "name": "font-weight",
                    "displayName": "字体加粗",
                    "defaultValue": "normal"
                }
            ]
        },
        {
            "name": ".ad-widget-table tbody tr td",
            "displayName": "行背景色",
            "rules": [
                {
                    "name": "background-color",
                    "displayName": "行背景色",
                    "defaultValue": "#FFFFFF"
                }
            ]
        },
        {
            "name": ".ad-widget-table tbody tr.ec-tr-bg td",
            "displayName": "奇数行背景色",
            "rules": [
                {
                    "name": "background-color",
                    "displayName": "奇数行背景色",
                    "defaultValue": "#D3E5FA"
                }
            ]
        },
        {
            "name": ".ad-widget-table",
            "displayName": "表格外边框",
            "rules": [
                {
                    "name": "border-right",
                    "displayName": "表格外边框（右）",
                    "defaultValue": "#BECCF1 1px solid"
                },
                {
                    "name": "border-bottom",
                    "displayName": "表格外边框（下）",
                    "defaultValue": "#BECCF1 1px solid"
                }
            ]
        },
        {
            "name": ".ad-widget-table table tr td, .ad-widget-table table tr.ec-tr-bg td",
            "displayName": "表TR边框",
            "rules": [
                {
                    "name": "border-top",
                    "displayName": "表TR边框（上）",
                    "defaultValue": "#BECCF1 1px solid"
                },
                {
                    "name": "border-left",
                    "displayName": "表TR边框（左）",
                    "defaultValue": "#BECCF1 1px solid"
                }
            ]
        },
        {
            "name": ".ad-widget-table table tbody tr td, .ad-widget-table table tbody tr.ec-tr-bg td",
            "displayName": "表tbody横边框（上）设置为none无横边框",
            "rules": [
                {
                    "name": "border-top",
                    "displayName": "表TR横向无边框",
                    "defaultValue": "#FFF 1px solid"
                }
            ]
        }

    ]
}
