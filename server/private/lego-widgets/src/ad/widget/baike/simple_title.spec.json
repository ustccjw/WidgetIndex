{
    "contents":["ad.widget.baike.SimpleTitle"],
    "userPrefs": [
        {
            "name": "brand_img",
            "displayName": "LOGO",
            "datatype": "UPLOAD",
            "rules": {
                "required": true
            },
            "extraAttr": {
                "width": 214,
                "height": 214,
                "maxSize": 50,
                "fileType": "IMAGE",
                "ext": "jpg,png"
            }
        },
        {
            "name": "brand_rcv_url",
            "displayName": "LOGO跳转地址",
            "tip": "点击LOGO时的跳转地址",
            "defaultValue": "http://",
            "datatype": "STRING",
            "rules": {
                "stringType": "URL",
                "required": true
            },
            "extraAttr": {}
        },
        {
            "name": "title",
            "displayName": "标题",
            "datatype": "STRING",
            "tip": "请输入商品标题,最多10个汉字",
            "rules": {
                "required": true,
                "maxByteLength": 20
            },
            "extraAttr": {}
        },
        {
            "name": "description_rcv_html",
            "displayName": "描述",
            "tip": "请输入商品描述信息,最多95个汉字",
            "datatype": "STRING",
            "multiline": "true",
            "rules": {
                "required": true,
                "maxByteLength": 190
            },
            "extraAttr": {}
        },
        {
            "name": "poster_img",
            "displayName": "海报",
            "datatype": "UPLOAD",
            "rules": {},
            "extraAttr": {
                "width": 275,
                "height": 246,
                "maxSize": 50,
                "fileType": "IMAGE",
                "ext": "jpg"
            }
        },
        {
            "name": "poster_rcv_url",
            "displayName": "跳转地址",
            "tip": "点击海报时的跳转地址",
            "defaultValue": "http://",
            "datatype": "STRING",
            "rules": {
                "stringType": "URL",
                "required": true
            },
            "extraAttr": {}
        },
        {
            "name": "options",
            "displayName": "信息条",
            "datatype": "LIST",
            "rules": {
                "minCount": 3
            },
            "items": [
                {
                    "name": "label",
                    "displayName": "标签",
                    "defaultValue": "",
                    "tip": "请输入标签,最多5个汉字",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 10
                    },
                    "extraAttr": {}
                },
                {
                    "name": "text",
                    "displayName": "文本",
                    "defaultValue": "",
                    "tip": "请输入文本,最多20个汉字",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 20
                    },
                    "extraAttr": {}
                }
            ]
        },
        {
            "name": "site",
            "displayName": "网站信息",
            "display": "toggle-block",
            "datatype": "OBJECT",
            "items": [
                {
                    "name": "label",
                    "displayName": "标签",
                    "defaultValue": "官方网站",
                    "datatype": "STRING",
                    "rules": {
                        "required": true
                    },
                    "extraAttr": {}
                },
                {
                    "name": "text",
                    "displayName": "链接文本",
                    "defaultValue": "",
                    "tip": "请输入链接文本,最多10个汉字",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 20
                    },
                    "extraAttr": {}
                },
                {
                    "name": "site_rcv_url",
                    "displayName": "链接地址",
                    "defaultValue": "http://",
                    "datatype": "STRING",
                    "rules": {
                        "stringType": "URL",
                        "required": true
                    },
                    "extraAttr": {}
                },
                {
                    "name": "tencent_weibo_rcv_url",
                    "displayName": "腾讯微博地址",
                    "defaultValue": "http://",
                    "datatype": "STRING",
                    "rules": {
                        "required": false,
                        "stringType": "URL"
                    },
                    "extraAttr": {}
                },
                {
                    "name": "sina_weibo_rcv_url",
                    "displayName": "新浪微博地址",
                    "defaultValue": "http://",
                    "datatype": "STRING",
                    "rules": {
                        "required": false,
                        "stringType": "URL"
                    },
                    "extraAttr": {}
                }
            ]
        }
    ]
