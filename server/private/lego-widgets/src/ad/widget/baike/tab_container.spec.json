            "datatype": "STRING",
            "rules": {},
            "extraAttr": {}
        },
        {
            "name": "options",
            "displayName": "系列",
            "datatype": "LIST",
            "rules": {
                "minCount": 4,
                "maxCount": 8
            },
            "items": [
                {
                    "name": "header",
                    "displayName": "名称",
                    "defaultValue": "",
                    "tip": "请输入系列名称,最多8个汉字",
                    "datatype": "STRING",
                    "rules": {
                        "required": true,
                        "maxByteLength": 16
                    },
                    "extraAttr": {}
                },
                {
                    "name": "items",
                    "displayName": "产品",
                    "datatype": "LIST",
                    "rules": {
                        "minCount": 1,
                        "maxCount": 3
                    },
                    "items": [
                        {
                            "name": "title",
                            "displayName": "名称",
                            "tip": "请输入产品名称,最多15个汉字",
                            "defaultValue": "",
                            "datatype": "STRING",
                            "rules": {
                                "required": true,
                                "maxByteLength": 30
                            },
                            "extraAttr": {}
                        },
                        {
                            "name": "desc",
                            "displayName": "描述",
                            "datatype": "LIST",
                            "rules": {
                                "minCount": 1
                            },
                            "items": [
                                {
                                    "name": "p",
                                    "displayName": "段落",
                                    "defaultValue": "",
                                    "tip": "一段商品的描述信息,所有的段落加起来请不要超过140个汉字。",
                                    "datatype": "STRING",
                                    "multiline": true,
                                    "rules": {
                                        "required": true,
                                        "maxByteLength": 280
                                    },
                                    "extraAttr": {}
                                }
                            ]
                        },
                        {
                            "name": "img",
                            "displayName": "图片",
                            "datatype": "UPLOAD",
                            "rules": {},
                            "extraAttr": {
                                "width": 200,
                                "height": 200,
                                "maxSize": 50,
                                "fileType": "IMAGE",
                                "ext": "jpg"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}