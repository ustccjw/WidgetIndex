            },
            "extraAttr": {
                "editorConfig": {
                    "tools": [
                        "link"
                    ]
                },
                "tagList": {
                    "a": {
                        "attributeRules": {
                            "target": {
                                "isRequired": false,
                                "isAllowNoValue": false,
                                "defaultValue": "_blank"
                            },
                            "href": {
                                "isRequired": true,
                                "isAllowNoValue": false,
                                "valueType": "url"
                            },
                            "kw": {
                                "isRequired": true,
                                "isAllowNoValue": false,
                                "defaultValue": "1"
                            }
                        },
                        "requiredAttributes": [
                            "target",
                            "href",
                            "kw"
                        ],
                        "isAllowNoContent": false,
                        "isSimpleTag": false
                    }
                }
            },
            "multiline": true
        },
