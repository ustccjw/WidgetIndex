<script>
// 初始化js
(function () {

var pushState = !!(window.history && window.history.pushState);

var url = '\/f?kw=%C3%C0%D6%AD%D4%B4&tp=0&pn='; // url pathname中固定的部分

$('#refresh').click(function (e) { // 贴子页刷新按钮事件绑定
	Path.refresh();
	e.preventDefault();
	return false;
});

function init () { // 初始化点击区域（翻页）事件
	$(document.getElementById('frs_list_pager')).click(function (e) {
		var t = e.target, key;
		if (t.tagName.toLowerCase() === 'a') {
			key = t.getAttribute('href', 2).replace(location.protocol + '//' + location.host, '');
			if (!pushState) {
				key = '/pn=' + key.match(/\d+$/);
			}
			Path.nav(key);
			e.preventDefault();
			return false;
		}
	});
}
init();

Path.map(pushState ? url+':num' : '/pn=:num').to(function (url, num, refresh) { // 建立key->action映射关系
	fetchPage(num, refresh);
});
Path.setDefaultAction(function (path, refresh) { // 找不到路由的时候的默认行为
	if (path.replace('#', '') === '' || refresh) {
		var num = path.match(/&pn=(\d+)/i);
		num = num ? num[1] : (cur_num === undefined ? 0 : cur_num);
		fetchPage(num, refresh);
	}

});

Path.listen(); // 开始监听url变化

var xhr = null, cur_num;

function fetchPage (num, refresh) { //  获取刷新部分数据
	var _url = url + num + '&apage=1&t=' + new Date().getTime();
    if (refresh) {
        _url += "&refresh=true";
    }

	if (xhr) {
		if (cur_num === num) {// 如果请求相同页 直接返回
			return;
		} else {
			xhr.abort();// 中断前一个请求
		}
	}

	cur_num = num;
	var notTop = ''; 

	xhr = $.get(_url, function (text) {
		xhr = null;
		if(notTop){ //不要滚动到顶部
			$(window).scrollTop($("#contet_wrap").offset().top-10);
		}else{
			if(!Path.is_star){
				gotoTop();
			}else{
				/*明星贴吧刷新后定位到贴子列表顶部*/
				$(window).scrollTop($("#contet_wrap").offset().top-10);
				Path.is_star = false;
			}
		}

		setTimeout(function () {
            var liveContainer = $('#threadListGroupCnt').add("#thread_topic").add("#thread_pic");
            if (refresh && liveContainer.size() > 0) {
                liveContainer.nextAll().remove();
                $('#content_leftList').append(text)
            } else {
                $('#content_leftList').empty().html(text); // 填充内容
            }
			init(); // 由于区域包括了翻页区域 所以需要重新绑定事件
		}, 0);

	}, 'text');
}

function gotoTop () { // 如果直接使用window.scrollTo(0, 0) 在safari下有时候会出现白页
	var top = $(window).scrollTop();
	if (top > 0) {
		window.scrollTo(0, 1);
		setTimeout(function () {
			window.scrollTo(0, 0);
		}, 0);
	}
}

})();

</script>
