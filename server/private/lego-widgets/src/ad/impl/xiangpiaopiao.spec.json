            {
                "name": "titletext",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 10,
                    "maxTextByteLength": 30,
                    "tagList": {
                        "em": {
                            "minTextByteLength": 6,
                            "maxTextByteLength": 10,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "请输入标题，限制5~15个汉字",
                "multiline": false
            },
            {
                "name": "titleurl_rcv_url",
                "displayName": "标题url",
                "defaultValue": "",
                "tip": "http://",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "stringType": "URL"
                },
                "extraAttr": {}
            },
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
                "tip": "描述为45-63个汉字，<a href=\"http://baidu.com\">链接词</a>最多5个，每个链接词最多6个汉字",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 90,
                    "maxTextByteLength": 126,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 12,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ]
                        }
                    }
                }
            },
