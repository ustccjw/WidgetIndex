                "rules": {
                    "required": true,
                    "minTextByteLength": 90,
                    "maxTextByteLength": 120,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 12,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
