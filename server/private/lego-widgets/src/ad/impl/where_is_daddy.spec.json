                "tip": "",
                "multiline": false
            },
            {
                "name": "logo",
                "displayName": "LOGO",
                "datatype": "OBJECT",
                "items": [
                    {
                        "name": "logo",
                        "displayName": "LOGO图片",
                        "defaultValue": "",
                        "datatype": "UPLOAD",
                        "tip": "http://dummyimage.com/91x91/",
                        "multiline": false,
                        "rules": {
                            "required": true
                        },
                        "extraAttr": {
                            "fileType": "IMAGE",
                            "width": 91,
                            "height": 91,
                            "maxSize": 10,
                            "ext": "jpg"
                        }
                    },
                    {
                        "name": "logo_rcv_url",
                        "displayName": "LOGO跳转地址",
                        "defaultValue": "",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "stringType": "URL",
                            "maxByteLength": 255
                        },
                        "tip": "",
                        "multiline": false
                    }
                ],
                "rules": {
                    "required": true
                }
            },
            {
                "name": "description",
                "displayName": "描述",
                "defaultValue": "",
