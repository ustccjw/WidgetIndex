        "items": [
            {
                "name": "title",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "multiline": false,
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 50,
                    "tagList": {
                        "em": {
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                }
            },
            {
                "name": "display_official_site_icon",
                "displayName": "显示官网标识",
                "defaultValue": true,
                "datatype": "BOOL",
                "display": "none"
