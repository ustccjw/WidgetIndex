[
    {
        "name": "title",
        "displayName": "标题",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "title",
                "displayName": "标题内容",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 50,
                    "tagList": {
                        "em": {
                            "minCount": 1,
                            "maxCount": 1,
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "multiline": false
            },
            {
                "name": "rcv_url",
                "displayName": "标题跳转网址",
                "datatype": "STRING",
                "defaultValue": "http://",
                "rules": {
                    "required": true,
                    "maxByteLength": 255,
                    "stringType": "URL"
                },
                "tip": "255字节以内"
            },
            {
                "name": "display_official_site_icon",
                "displayName": "是否显示“官网”字样",
                "defaultValue": true,
                "datatype": "BOOL",
                "rules": {}
            }
        ]
    },
    {
        "name": "image_slide_show",
        "displayName": "图片轮播",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "options",
                "displayName": "图片",
                "datatype": "LIST",
                "rules": {
                    "minCount": 2,
                    "maxCount": 5
                },
                "items": [
                    {
                        "name": "img_src",
                        "displayName": "图片地址",
                        "tip": "http://dummyimage.com/254x143",
                        "datatype": "UPLOAD",
                        "rules": {
                            "required": true
                        },
                        "extraAttr": {
                            "fileType": "IMAGE",
                            "width": 254,
                            "height": 143,
                            "maxSize": 30,
                            "ext": "jpg,jpeg"
                        }
                    },
                    {
                        "name": "img_rcv_url",
                        "displayName": "图片跳转地址",
                        "tip": "http://",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "stringType": "URL",
                            "maxByteLength": 255
                        }
                    }
                ]
            }
        ]
    },
    {
        "name": "small_head",
        "displayName": "右侧描述",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 80,
                    "maxTextByteLength": 100,
                    "tagList": {
                        "a": {
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12,
                            "minCount": 0,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ]
                        }
                    }
                }
            },
            {
                "name": "site",
                "displayName": "显示url",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 20
                },
                "extraAttr": {},
                "multiline": false
            }
        ],
        "rules": {}
    },
    {
        "name": "right_buttons",
        "displayName": "右侧按钮",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "options",
                "displayName": "按钮",
                "datatype": "LIST",
                "rules": {
                    "minCount": 4,
                    "maxCount": 4
                },
                "items": [
                    {
                        "name": "text",
                        "displayName": "文本",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "maxByteLength": 16
                        }
                    },
                    {
                        "name": "rcv_url",
                        "displayName": "跳转地址",
                        "tip": "http://",
                        "datatype": "STRING",
                        "rules": {
                            "stringType": "URL",
                            "maxByteLength": 255,
                            "required": true
                        }
                    }
                ]
            }
        ]
    },
    {
        "name": "tab_container",
        "displayName": "多标签页模块",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "interval_time",
                "displayName": "图片播放间隔",
                "nameTip": "(毫秒)",
                "defaultValue": "40000000",
                "datatype": "STRING",
                "display": "none",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "hover_time",
                "displayName": "鼠标覆盖按钮延时触发",
                "nameTip": "(毫秒)",
                "defaultValue": "300",
                "datatype": "HIDDEN",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "width",
                "displayName": "tab container width",
                "defaultValue": "518",
                "datatype": "HIDDEN",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "li_border_width",
                "displayName": "li_border_width",
                "defaultValue": "0",
                "datatype": "HIDDEN",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "li_margin",
                "displayName": "li_margin",
                "defaultValue": "0",
                "datatype": "HIDDEN",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "default_index",
                "displayName": "default_index",
                "defaultValue": "0",
                "datatype": "HIDDEN",
                "rules": {
                    "stringType": "NUMBER"
                },
                "extraAttr": {
                    "filters": "float"
                }
            },
            {
                "name": "options",
                "displayName": "Tab",
                "datatype": "LIST",
                "rules": {
                    "minCount": 3,
                    "maxCount": 6
                },
                "items": [
                    {
                        "name": "tab_title",
                        "displayName": "Tab名称",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "maxByteLength": 20,
                            "minByteLength": 1
                        }
                    },
                    {
                        "name": "image",
                        "displayName": "产品图片",
                        "datatype": "OBJECT",
                        "display": "toggle-block",
                        "items": [
                            {
                                "name": "image_url",
                                "displayName": "图片",
                                "defaultValue": "http://www.dummyimage.com/254x96/",
                                "datatype": "UPLOAD",
                                "rules": {
                                    "required": true
                                },
                                "extraAttr": {
                                    "fileType": "IMAGE",
                                    "width": 254,
                                    "height": 96,
                                    "maxSize": 30,
                                    "ext": "jpg"
                                }
                            },
                            {
                                "name": "image_rcv_url",
                                "displayName": "图片跳转地址",
                                "tip": "http://",
                                "datatype": "STRING",
                                "rules": {
                                    "stringType": "URL",
                                    "maxByteLength": 255,
                                    "required": true
                                }
                            }
                        ]
                    },
                    {
                        "name": "color_list",
                        "displayName": "产品介绍",
                        "datatype": "OBJECT",
                        "display": "toggle-block",
                        "items": [
                            {
                                "name": "options",
                                "displayName": "产品介绍",
                                "datatype": "LIST",
                                "rules": {
                                    "minCount": 2,
                                    "maxCount": 2
                                },
                                "items": [
                                    {
                                        "name": "text",
                                        "displayName": "产品标题",
                                        "datatype": "STRING",
                                        "rules": {
                                            "required": true,
                                            "minByteLength": 8,
                                            "maxByteLength": 34
                                        }
                                    },
                                    {
                                        "name": "rcv_url",
                                        "displayName": "标题跳转地址",
                                        "tip": "http://",
                                        "datatype": "STRING",
                                        "rules": {
                                            "stringType": "url",
                                            "maxByteLength": 255,
                                            "required": true
                                        }
                                    },
                                    {
                                        "name": "desc",
                                        "displayName": "产品描述",
                                        "datatype": "STRING",
                                        "rules": {
                                            "required": false,
                                            "minByteLength": 8,
                                            "maxByteLength": 28
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    },
