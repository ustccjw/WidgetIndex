[
    {
        "name": "title",
        "displayName": "标题",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "title",
                "displayName": "标题内容",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 10,
                    "maxTextByteLength": 30,
                    "tagList": {
                        "em": {
                            "minCount": 1,
                            "maxCount": 1,
                            "minTextByteLength": 1,
                            "maxTextByteLength": 12
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "5~15个汉字,必须有一个飘红词(6个汉字以内)",
                "multiline": false
            },
            {
                "name": "rcv_url",
                "displayName": "标题跳转网址",
                "datatype": "STRING",
                "defaultValue": "http://",
                "rules": {
                    "required": true,
                    "maxByteLength": 255,
                    "stringType": "URL"
                },
                "tip": "255字节以内"
            },
            {
                "name": "display_official_site_icon",
                "displayName": "是否显示“官网”字样",
                "datatype": "BOOL",
                "defaultValue": true,
                "display": "none",
                "rules": {},
                "extraAttr": {
                    "filters": "bool"
                }
            },
            {
                "name": "logoimg",
                "displayName": "品牌 Logo",
                "datatype": "OBJECT",
                "display": "toggle-block",
                "items": [
                    {
                        "name": "logoimg",
                        "displayName": "图片",
                        "defaultValue": "http://dummyimage.com/110x15",
                        "datatype": "UPLOAD",
                        "rules": {
                            "required": false
                        },
                        "extraAttr": {
                            "fileType": "IMAGE",
                            "width": 110,
                            "height": 15,
                            "maxSize": 10,
                            "ext": "jpg"
                        }
                    },
                    {
                        "name": "logo_rcv_url",
                        "displayName": "跳转网址",
                        "datatype": "STRING",
                        "defaultValue": "",
                        "rules": {
                            "required": false,
                            "maxByteLength": 255,
                            "stringType": "URL"
                        },
                        "tip": "255字节以内"
                    }
                ]
            } 
        ]
    },
    {
        "name": "head",
        "displayName": "描述",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
                "tip": "描述为40~50个汉字，<a href=\"http://baidu.com\">链接词</a>最多5个，每个链接词最多6个汉字",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 80,
                    "maxTextByteLength": 100,
                    "tagList": {
                        "a": {
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ]
                        }
                    }
                }
            },
            {
                "name": "site",
                "displayName": "显示url",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 20
                },
                "extraAttr": {},
                "tip": "<=20字节",
                "multiline": false
            }
        ],
        "rules": {}
    },
