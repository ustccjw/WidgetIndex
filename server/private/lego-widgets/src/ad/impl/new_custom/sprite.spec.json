[
    {
        "name": "title",
        "displayName": "标题",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "title",
                "displayName": "标题内容",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 50,
                    "tagList": {
                        "em": {
                            "minCount": 1,
                            "maxCount": 1,
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "multiline": false
            },
            {
                "name": "rcv_url",
                "displayName": "标题跳转网址",
                "datatype": "STRING",
                "defaultValue": "http://",
                "rules": {
                    "required": true,
                    "maxByteLength": 255,
                    "stringType": "URL"
                },
                "tip": "255字节以内"
            },
            {
                "name": "display_official_site_icon",
                "displayName": "是否显示“官网”字样",
                "defaultValue": true,
                "datatype": "BOOL",
                "rules": {}
            }
        ]
    },
