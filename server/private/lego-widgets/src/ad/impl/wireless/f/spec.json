[
    {
        "name": "head",
        "displayName": "标题描述",
        "display": "toggle-block",
        "tip": "请参照各版式截断字数，按照规范填写主标题、按钮文字，如果太长则会截断造成语意不明",
        "datatype": "OBJECT",
        "items": [
            {
                "name": "video_title_logo",
                "displayName": "标题上的LOGO图片，仅智能机显示",
                "datatype": "UPLOAD",
                "tip": "http://dummyimage.com/34x34",
                "multiline": false,
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE",
                    "width": 36,
                    "height": 36,
                    "maxSize": 10,
                    "ext": "jpg,png"
                }
            },
            {
                "name": "title",
                "displayName": "主标题",
                "tip": "13汉字内，<em>飘红词</em>最多6汉字",
                "datatype": "HTML",
                "multiline": false,
                "rules": {
                    "required": true,
                    "maxTextByteLength": 26,
                    "tagList": {
                        "em": {
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                }
            },
            {
                "name": "display_official_site_icon",
                "displayName": "显示官网标识",
                "defaultValue": true,
                "datatype": "BOOL",
                "display": "none"
            },
            {
                "name": "rcv_url",
                "displayName": "主标题指向URL",
                "defaultValue": "http://",
                "descriptionTip": "用于无线设备展现，URL推荐填写WAP地址",
                "datatype": "STRING",
                "multiline": false,
                "rules": {
                    "required": true,
                    "stringType": "URL"
                }
            },
            {
                "name": "video_image",
                "displayName": "视频预览图片",
                "datatype": "UPLOAD",
                "tip": "http://dummyimage.com/172x128",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "width": 172,
                    "height": 128,
                    "maxSize": 10,
                    "ext": "jpg,jpeg,png"
