            },
            {
                "name": "video_src",
                "displayName": "视频FLV文件（可选）",
                "datatype": "UPLOAD",
                "extraAttr": {
                    "fileType": "FLV",
                    "maxSize": 20480,
                    "ext": "mp4,m4v"
                }
            },
            {
                "name": "video_rcv_url",
                "displayName": "视频指向URL",
                "defaultValue": "http://www.baidu.com/video/jump",
                "descriptionTip": "用于无线设备展现，URL推荐填写WAP地址",
                "datatype": "HIDDEN",
                "multiline": false,
                "rules": {
                    "required": true,
                    "stringType": "URL"
                }
            },
            {
                "name": "video_desc",
                "displayName": "视频长度，11:00:00（可选）",
