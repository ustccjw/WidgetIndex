                "multiline": false,
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE",
                    "width": 34,
                    "height": 34,
                    "maxSize": 10,
                    "ext": "jpg,png"
                }
            },
            {
                "name": "logo_rcv_url",
                "displayName": "LOGO指向URL",
                "defaultValue": "http://",
                "descriptionTip": "用于无线设备展现，URL推荐填写WAP地址",
                "datatype": "STRING",
                "multiline": false,
                "rules": {
                    "required": true,
                    "stringType": "URL"
                }
            },
            {
                "name": "description",
                "displayName": "内容描述",
                "tip": "可填写26~34个汉字",
                "datatype": "STRING",
                "multiline": true,
                "rules": {
                    "required": true,
                    "minByteLength": 52,
                    "maxByteLength": 68
                }
            },
            {
                "name": "site",
                "displayName": "官方网站",
                "tip": "最多22个字节",
                "datatype": "STRING",
                "multiline": false,
                "rules": {
                    "required": true,
                    "maxByteLength": 22
                }
            }
        ]
    },
