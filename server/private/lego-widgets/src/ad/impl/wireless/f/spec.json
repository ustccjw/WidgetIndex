    {
        "name": "tel_button",
        "displayName": "电话区",
        "display": "toggle-block",
        "datatype": "OBJECT",
        "multiline": false,
        "items": [
            {
                "name": "button",
                "displayName": "客服电话按钮",
                "display": "toggle-block",
                "datatype": "LIST",
                "multiline": false,
                "rules": {
                    "minCount": 0,
                    "maxCount": 1
                },
                "extraAttr": {
                    "initialCount": 1
                },
                "items": [
                    {
                        "name": "tel_name",
                        "displayName": "热线名称（例如：客服专线）",
                        "defaultValue": "客服电话",
                        "tip": "请填写2~5个汉字",
                        "datatype": "STRING",
                        "multiline": false,
                        "rules": {
                            "required": true,
                            "minByteLength": 4,
                            "maxByteLength": 10
                        }
                    },
                    {
                        "name": "tel",
                        "displayName": "热线号码",
                        "defaultValue": "",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "phoneNumber": true
                        },
                        "extraAttr": {
                            "extensions": [
                                {
                                    "name": "format",
