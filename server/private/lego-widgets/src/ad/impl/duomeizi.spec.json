[
    {
        "name": "head",
        "displayName": "头部模块",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "logo",
                "displayName": "LOGO",
                "datatype": "UPLOAD",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "width": 110,
                    "height": 110,
                    "maxSize": 10,
                    "ext": "jpg,jpeg,png,gif"
                }
            },
            {
                "name": "logo_rcv_url",
                "displayName": "LOGO跳转地址",
                "tip": "http://",
                "datatype": "STRING",
                "rules": {
                    "stringType": "URL",
                    "maxByteLength": 255
                },
                "extraAttr": {}
            },
            {
                "name": "title",
                "displayName": "标题",
                "datatype": "HTML",
                "multiline": false,
                "tip": "标题为5~15个汉字，核心<em>飘红词</em>最多可选6个汉字",
                "rules": {
                    "maxTextByteLength": 30,
                    "minTextByteLength": 10,
                    "required": true,
                    "tagList": {
                        "em": {
                            "maxCount": 1,
                            "maxTextByteLength": 20,
                            "minCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                }
            },
            {
                "name": "rcv_url",
                "displayName": "标题URL",
                "tip": "http://",
                "datatype": "STRING",
                "rules": {
                    "stringType": "URL",
                    "maxTextByteLength": 255,
                    "required": true
                },
                "extraAttr": {}
            },
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "tip": "描述为45~95个汉字，<a href=\"http://fake.com\">链接词</a>最多5个，每个链接词最多5个汉字",
                "datatype": "HTML",
                "multiline": true,
                "rules": {
                    "maxTextByteLength": 190,
                    "minTextByteLength": 90,
                    "required": true,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 20,
                            "maxCount": 5
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ],
                            "attributeRules": {
                                "href": {
                                    "isRequired": true,
                                    "isAllowNoValue": false,
                                    "valueType": "url"
                                },
                                "target": {
                                    "isRequired": false,
                                    "isAllowNoValue": false,
                                    "defaultValue": "_blank"
                                }
                            }
                        }
                    }
                }
            },
            {
                "name": "site",
                "displayName": "官方网站",
                "tip": "最多可填写24个字节",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 24
                },
                "extraAttr": {}
            }
        ]
    },
