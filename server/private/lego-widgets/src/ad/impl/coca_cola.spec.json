            {
                "name": "title",
                "displayName": "标题",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 50,
                    "tagList": {
                        "em": {
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
