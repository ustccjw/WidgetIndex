    {
        "name": "table",
        "displayName": "表格模块",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "rules": {
            "jsRule": "function abc(table){function getByteLength(source) { return String(source).replace(/[^\\x00-\\xff]/g, 'ci').length; } var data = table['tbody']; var rows = data.length, cols = data[0].length;var maxByteLen = [12, 16, 24][5-cols];for (var i=0;i<rows;i++) for(var j=0;j<cols;j++) {if (getByteLength(data[i][j].text) > maxByteLen) { return '表格为' + cols + '列时, 每格最多' + maxByteLen + '个字节(' + maxByteLen/2 + '个汉字)'; } } }"
        },
