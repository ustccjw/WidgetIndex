                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 60,
                    "tagList": {
                        "em": {
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "请输入标题，最少6个汉字，最多30个汉字,飘红词最多6个汉字",
                "multiline": false
            },
            {
                "name": "logo_rcv_url",
                "displayName": "LOGO跳转地址",
                "defaultValue": "",
                "datatype": "STRING",
