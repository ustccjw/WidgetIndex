                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 190,
                    "maxTextByteLength": 230,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 20,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ]
                        }
                    }
                }
            },
            {
                "name": "site",
