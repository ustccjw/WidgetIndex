                "extraAttr": {},
                "tip": "",
                "multiline": false
            },
            {
                "name": "logo",
                "displayName": "LOGO",
                "defaultValue": "",
                "datatype": "UPLOAD",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE",
                    "width": 90,
                    "height": 90,
                    "maxSize": 10,
                    "ext": "jpg"
                }
            },
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
