            "required": true,
            "minTextByteLength": 10,
            "maxTextByteLength": 30,
            "tagList": {
                "em": {
                    "minTextByteLength": 2,
                    "maxTextByteLength": 12,
                    "minCount": 1,
                    "maxCount": 1
                }
            }
        },
        "extraAttr": {
            "editorConfig": {
                "tools": [
                    "emphasize"
                ]
            },
            "tagList": {
                "em": {
                    "isAllowNoContent": false,
                    "isSimpleTag": false
                }
            }
