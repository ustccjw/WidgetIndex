        "datatype": "HTML",
        "rules": {
            "required": true,
            "minTextByteLength": 90,
            "maxTextByteLength": 130,
            "tagList": {
                "a": {
                    "maxTextByteLength": 12,
                    "maxCount": 5
                }
            }
        },
        "multiline": true,
        "extraAttr": {
            "editorConfig": {
                "tools": [
                    "link"
                ]
            },
            "tagList": {
                "a": {
                    "attributeRules": {
                        "href": {
                            "isAllowNoValue": false,
                            "isRequired": true,
                            "valueType": "url"
                        },
                        "target": {
                            "defaultValue": "_blank",
                            "isAllowNoValue": false,
                            "isRequired": false
                        }
                    },
                    "isAllowNoContent": false,
                    "isSimpleTag": false,
                    "requiredAttributes": [
                        "target",
                        "href"
                    ]
                }
            }
        }
