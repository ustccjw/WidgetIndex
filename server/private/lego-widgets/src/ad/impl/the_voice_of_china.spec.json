[
    {
        "name": "h1",
        "displayName": "头部",
        "display": "toggle-block",
        "datatype": "OBJECT",
        "items": [
            {
                "name": "title",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "multiline": false,
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 50,
                    "tagList": {
                        "em": {
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                }
            },
            {
                "name": "display_official_site_icon",
                "displayName": "显示官网标识",
                "defaultValue": true,
                "datatype": "BOOL",
                "display": "none"
            },
            {
                "name": "rcv_url",
                "displayName": "标题URL",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "stringType": "URL",
                    "maxByteLength": 255
                },
                "tip": "",
                "multiline": false
            },
            {
                "name": "logo",
                "displayName": "左上大图",
                "datatype": "OBJECT",
                "items": [
                    {
                        "name": "logo",
                        "displayName": "图片",
                        "datatype": "UPLOAD",
                        "tip": "http://dummyimage.com/254x143/",
                        "multiline": false,
                        "rules": {
                            "required": true
                        },
                        "extraAttr": {
                            "fileType": "IMAGE",
                            "width": 254,
                            "height": 143,
                            "maxSize": 30,
                            "ext": "jpg"
                        }
                    },
                    {
                        "name": "logo_rcv_url",
                        "displayName": "图片链接",
                        "defaultValue": "",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "stringType": "URL",
                            "maxByteLength": 255
                        },
                        "tip": "",
                        "multiline": false
                    }
                ],
                "rules": {
                    "required": true
                }
            },
            {
                "name": "description",
                "displayName": "description",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "minByteLength": 62,
                    "maxByteLength": 70
                },
                "multiline": true
            },
            {
                "name": "site",
                "displayName": "官方网站",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 24
                },
                "multiline": false
            }
        ]
    },
    {
        "name": "weibo",
        "displayName": "微博",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "follow_imgsrc",
                "displayName": "follow_imgsrc",
                "defaultValue": "http://ecma.bdimg.com/adtest/d29a0e9922c9986e478155ba2fe3a61b.jpg",
                "datatype": "UPLOAD",
                "display": "none",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE"
                }
            },
            {
                "name": "verify_img",
                "displayName": "verify_img",
                "defaultValue": "http://ecma.bdimg.com/adtest/fc29dbeea7be67da54a68e0f65fb3a50.gif",
                "datatype": "UPLOAD",
                "display": "none",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE"
                }
            },
            {
                "name": "no_fieldset",
                "displayName": "no_fieldset",
                "defaultValue": true,
                "datatype": "BOOL",
                "display": "none"
            },
            {
                "name": "is_display_icon",
                "displayName": "is_display_icon",
                "defaultValue": false,
                "datatype": "BOOL",
                "display": "none"
            },
            {
                "name": "weibo_length",
                "displayName": "显示时截取长度(字节)",
                "datatype": "STRING",
                "defaultValue": "76",
                "rules": {
                    "required": true,
                    "stringType": "NUMBER"
                }
            },
            {
                "name": "type",
                "displayName": "微博平台",
                "defaultValue": "sina",
                "datatype": "ENUM",
                "extraAttr": {},
                "enumValues": [
                    {
                        "value": "sina",
                        "displayValue": "新浪微博"
                    },
                    {
                        "value": "qqweibo",
                        "displayValue": "腾讯微博"
                    }
                ],
                "rules": {
                    "required": true
                }
            },
            {
                "name": "id",
                "displayName": "微博ID",
                "datatype": "STRING",
                "defaultValue": "2525235360",
                "tip": "打开自己的微博主页，网址上那串数字就是你的微博ID",
                "rules": {
                    "required": true,
                    "stringType": "NUMBER"
                }
            },
            {
                "name": "name",
                "displayName": "微博名",
                "datatype": "STRING",
                "defaultValue": "",
                "rules": {}
            },
            {
                "name": "is_display_icon",
                "displayName": "is_display_icon",
                "defaultValue": false,
                "datatype": "BOOL",
                "display": "none",
                "rules": {}
            }
        ]
    },
    {
        "name": "img_list",
        "displayName": "视频列表",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "options",
                "displayName": "视频图片",
                "datatype": "LIST",
                "items": [
                    {
                        "name": "img_url",
                        "displayName": "图片",
                        "tip": "http://dummyimage.com/121x91",
                        "datatype": "UPLOAD",
                        "rules": {
                            "required": true
                        },
                        "extraAttr": {
                            "fileType": "IMAGE",
                            "width": 121,
                            "height": 91,
                            "maxSize": 10,
                            "ext": "jpg"
                        }
                    },
                    {
                        "name": "rcv_url",
                        "displayName": "跳转地址",
                        "defaultValue": "",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "maxByteLength": 255,
                            "stringType": "URL"
                        }
                    },
                    {
                        "name": "text",
                        "displayName": "文本",
                        "tip": "yyyy-mm-dd期",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "regex": {
                                "pattern": "^20\\d{2}-\\d{2}-\\d{2}期$",
                                "modifiers": "i",
                                "errorText": "格式错误"
                            }
                        }
                    }
                ],
                "rules": {
                    "minCount": 4,
                    "maxCount": 4
                },
                "extraAttr": {
                    "initialCount": 4
                }
            }
        ],
        "rules": {}
    },
    {
        "name": "button_group",
        "displayName": "底部链接",
        "display": "toggle-block",
        "datatype": "OBJECT",
        "items": [
            {
                "name": "options",
                "displayName": "按钮",
                "datatype": "LIST",
                "items": [
                    {
                        "name": "text",
                        "displayName": "链接文本",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "maxByteLength": 10
                        }
                    },
                    {
                        "name": "rcv_url",
                        "displayName": "链接地址",
                        "tip": "http://",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "stringType": "URL",
                            "maxByteLength": 255
                        }
                    }
                ],
                "rules": {
                    "minCount": 4,
                    "maxCount": 4
                }
            },
            {
                "name": "fixed",
                "displayName": "按钮5",
                "display": "toggle-block",
                "datatype": "OBJECT",
                "items": [
                    {
                        "name": "text",
                        "displayName": "链接文本",
                        "defaultValue": "更多视频",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "maxByteLength": 10
                        },
                        "extraAttr": {
                            "readOnly": true
                        }
                    },
                    {
                        "name": "rcv_url",
                        "displayName": "链接地址",
                        "tip": "http://",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "stringType": "URL",
                            "maxByteLength": 255
                        }
                    }
                ]
            }
        ]
    }
]