[
    {
        "name": "head",
        "displayName": "头部模块",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "title",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 60,
                    "tagList": {
                        "em": {
                            "minTextByteLength": 6,
                            "maxTextByteLength": 10,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "请输入标题，最少6个汉字，最多30个汉字",
                "multiline": false
            },
            {
                "name": "rcv_url",
                "displayName": "标题链接",
                "tip": "http://",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 255
                },
                "extraAttr": {},
                "multiline": false
            },
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
                "tip": "描述为65~83个汉字，<a href=\"http://baidu.com\">链接词</a>最多5个，每个链接词最多6个汉字",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 190,
                    "maxTextByteLength": 230,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 12,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": ["link"]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": ["target", "href"]
                        }
                    }   
                }
            },
            {
                "name": "logo",
                "displayName": "Logo图片",
                "defaultValue": "",
                "datatype": "UPLOAD",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "maxSize": 5,
                    "width": 90,
                    "height": 90,
                    "ext": "jpg,jpeg,png,gif"
                }
            },
            {
                "name": "logo_rcv_url",
                "displayName": "Logo跳转地址",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "stringType": "URL"
                },
                "extraAttr": {},
                "tip": "http://",
                "multiline": false
            },
            {
                "name": "site",
                "displayName": "官网地址",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 40
                },
                "extraAttr": {},
                "tip": "www.baidu.com",
                "multiline": false
            }
        ],
        "rules": {}
    },
