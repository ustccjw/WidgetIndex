    {
        "name": "head",
        "displayName": "右侧头部",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "titletext",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 10,
                    "maxTextByteLength": 26,
                    "tagList": {
                        "em": {
                            "minTextByteLength": 2,
                            "maxTextByteLength": 12,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "请输入标题，5~13个汉字,飘红词最多6个汉字",
                "multiline": false
            },
            {
                "name": "titleurl_rcv_url",
                "displayName": "标题跳转地址",
                "defaultValue": "http://",
                "datatype": "STRING",
                "rules": {
                    "stringType": "URL",
                    "required": true,
                    "maxByteLength": 255
                },
                "extraAttr": {},
                "tip": "",
                "multiline": false
            },
            {
                "name": "display_official_site_icon",
                "displayName": "显示官网标识",
                "defaultValue": true,
                "datatype": "BOOL"
            },
            {
                "name": "description_rcv_html",
                "displayName": "描述",
                "defaultValue": "",
                "tip": "描述为45~70个汉字，<a href=\"http://baidu.com\">链接词</a>最多5个，每个链接词最多6个汉字",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 90,
                    "maxTextByteLength": 140,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 12,
                            "maxCount": 5
                        }
                    }
                },
                "multiline": true,
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
                            "attributeRules": {
                                "href": {
                                    "isAllowNoValue": false,
                                    "isRequired": true,
                                    "valueType": "url"
                                },
                                "target": {
                                    "defaultValue": "_blank",
                                    "isAllowNoValue": false,
                                    "isRequired": false
                                }
                            },
                            "isAllowNoContent": false,
                            "isSimpleTag": false,
                            "requiredAttributes": [
                                "target",
                                "href"
                            ]
                        }
                    }
                }
            },
            {
                "name": "site",
                "displayName": "显示url",
                "defaultValue": "",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 24
                },
                "extraAttr": {},
                "tip": "",
                "multiline": false
            }
        ],
        "rules": {}
    },
