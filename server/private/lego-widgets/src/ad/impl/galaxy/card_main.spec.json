[
    {
        "name": "ader_info",
        "displayName": "ader_info",
        "datatype": "HIDDEN",
        "rules": {
            "required": true,
            "regex": {
                "pattern": "^(http:\/\/10.)|(http:\/\/gx.baidu.com)",
                "modifiers": "i",
                "errorText": "不是合法的URL"
            }
        }
    },
    {
        "name": "head",
        "displayName": "标题区",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "display_official_site_icon",
                "displayName": "显示官网标识",
                "defaultValue": true,
                "datatype": "BOOL",
                "display": "none"
            },
            {
                "name": "title",
                "displayName": "标题",
                "defaultValue": "",
                "datatype": "HTML",
                "rules": {
                    "required": true,
                    "minTextByteLength": 12,
                    "maxTextByteLength": 60,
                    "tagList": {
                        "em": {
                            "maxTextByteLength": 20,
                            "minTextByteLength": 4,
                            "minCount": 1,
                            "maxCount": 1
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "emphasize"
                        ]
                    },
                    "tagList": {
                        "em": {
                            "isAllowNoContent": false,
                            "isSimpleTag": false
                        }
                    }
                },
                "tip": "标题，最少6个汉字，最多30个汉字,飘红词最多10个汉字"
            },
            {
                "name": "rcv_url",
                "displayName": "标题跳转地址",
                "defaultValue": "http://",
                "datatype": "STRING",
                "rules": {
                    "stringType": "URL",
                    "required": true,
                    "maxByteLength": 1024
                }
            }
        ]
    },
    {
        "name": "main",
        "displayName": "公司信息",
        "datatype": "OBJECT",
        "display": "toggle-block",
        "items": [
            {
                "name": "logo_img",
                "displayName": "公司LOGO",
                "tip": "http://dummyimage.com/121x121/ccc/999",
                "datatype": "UPLOAD",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE",
                    "width": 121,
                    "height": 121,
                    "maxSize": 10,
                    "ext": "jpg,png"
                }
            },
            {
                "name": "logo_rcv_url",
                "displayName": "LOGO跳转地址",
                "defaultValue": "http://",
                "datatype": "STRING",
                "rules": {
                    "stringType": "URL",
                    "required": true,
                    "maxByteLength": 1024
                }
            },
            {
                "name": "description",
                "displayName": "描述1",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 52,
                    "minByteLength": 4
                },
                "tip": "2-26个汉字"
            },
            {
                "name": "business",
                "displayName": "描述2",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 52,
                    "minByteLength": 4
                },
                "tip": "2-26个汉字"
            },
            {
                "name": "full_name",
                "displayName": "公司简介",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 46,
                    "minByteLength": 4
                },
                "tip": "2-23个汉字"
            },
            {
                "name": "address",
                "displayName": "公司地址",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 60,
                    "minByteLength": 4
                },
                "tip": "2-30个汉字"
            },
            {
                "name": "show_maps",
                "displayName": "显示地图icon",
                "defaultValue": true,
                "datatype": "BOOL"
            },
            {
                "name": "links",
                "displayName": "短链",
                "datatype": "LIST",
                "rules": {
                    "minCount": 4,
                    "maxCount": 4
                },
                "items": [
                    {
                        "name": "text",
                        "displayName": "短链内容",
                        "datatype": "STRING",
                        "rules": {
                            "required": true,
                            "minByteLength": 4,
                            "maxByteLength": 26
                        },
                        "tip": "短链内容2-13个汉字"
                    },
                    {
                        "name": "rcv_url",
                        "displayName": "短链跳转地址",
                        "defaultValue": "http://",
                        "datatype": "STRING",
                        "rules": {
                            "stringType": "URL",
                            "required": true,
                            "maxByteLength": 1024
                        }
                    }
                ]
            },
            {
                "name": "official_site",
                "displayName": "公司官方网站",
                "datatype": "STRING",
                "rules": {
                    "required": true,
                    "maxByteLength": 40
                },
                "extraAttr": {
                    "tags": [
                        "SHOW_URL"
                    ]
                },
                "tip": "www.baidu.com"
            },
            {
                "name": "sign",
                "displayName": "sign",
                "datatype": "HIDDEN",
                "nameTip": "",
                "descriptionTip": "这里应该显示正确的vsign",
                "extraAttr": {
                    "tags": [
                        "VSIGN"
                    ]
                },
                "multiline": false,
                "display": ""
            },
            {
                "name": "ps_select",
                "displayName": "分享网址",
                "datatype": "OBJECT",
                "display": "none",
                "items": []
            }
        ]
    }
]