                "extraAttr": {},
                "tip": "",
                "multiline": false
            },
            {
                "name": "logo",
                "displayName": "LOGO",
                "tip": "http://dummyimage.com/90x90/ddd/333.jpg",
                "datatype": "UPLOAD",
                "rules": {
                    "required": true
                },
                "extraAttr": {
                    "fileType": "IMAGE",
                    "width": 90,
                    "height": 90,
                    "maxSize": 5,
                    "ext": "jpg,png,gif"
