                "datatype": "HTML",
                "multiline": true,
                "rules": {
                    "maxTextByteLength": 130,
                    "minTextByteLength": 90,
                    "required": true,
                    "tagList": {
                        "a": {
                            "maxTextByteLength": 20,
                            "maxCount": 5
                        }
                    }
                },
                "extraAttr": {
                    "editorConfig": {
                        "tools": [
                            "link"
                        ]
                    },
                    "tagList": {
                        "a": {
